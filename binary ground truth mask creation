save_dir = '/users/aymanasaria/video_3/video_3_masks'
os.makedirs(save_dir, exist_ok=True)

num_masks_created = 0  # Initialize the count

for image_filename, tool_coordinates in data:
    segmentation_mask = create_segmentation_mask((1080, 1920), tool_coordinates)
    
    mask_save_path = os.path.join(save_dir, image_filename.replace('.jpg', '_mask.png'))
    cv2.imwrite(mask_save_path, segmentation_mask)
    
    num_masks_created += 1  

print("Number of masks created:", num_masks_created)
