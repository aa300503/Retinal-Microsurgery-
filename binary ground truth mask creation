# making masks as greyscale label with pixels from 0 to 1 only 
import cv2
import numpy as np
import os

def create_segmentation_mask(image_shape, tool_coordinates):
    mask = np.zeros(image_shape[:2], dtype=np.uint8)
    tool_coordinates = [tuple(map(int, coord)) for coord in tool_coordinates]
    cv2.drawContours(mask, [np.array(tool_coordinates).reshape((-1, 1, 2))], -1, (1,), thickness=cv2.FILLED)
    return mask


